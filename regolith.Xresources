## Mod and win keys
i3-wm.mod: Mod1
i3-wm.alt: Mod4

## i3-wm
i3-wm.window.border.size: 0
i3-wm.floatingwindow.border.size: 0
i3-wm.gaps.inner.size: 24
i3-wm.gaps.outer.size: 0
i3-wm.gaps.smart: inverse_outer
i3-wm.gaps.focus_follows_mouse: yes
i3-wm.gaps.workspace_auto_back_and_forth: yes
i3-wm.font: pango:DejaVu Sans Mono 8

## terminal
gnome.terminal.font: DejaVu Sans Mono 12

## i3 bar
i3-wm.bar.position: top
i3-wm.bar.font: pango:DejaVu Sans Mono 12
i3-wm.bar.trayoutput: primary
i3-wm.bar.stripworkspacenumbers: yes

## rofi
rofi.font: DejaVuSansMono 12

## i3xrocks
i3xrocks.date.format2: + %A %e %B %Y  %H:%M
i3xrocks.label.notify.error: 
i3xrocks.label.notify.none: 
i3xrocks.label.notify.some: 
i3xrocks.label.time: 
i3xrocks.label.memory: 
i3xrocks.label.cpu: 
i3xrocks.label.wifi:  
i3xrocks.label.up: 
i3xrocks.label.dn: 
i3xrocks.value.font: DejaVu Sans Mono 12

